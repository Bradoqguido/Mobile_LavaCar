<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.root.jefersonguido.lavacar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icons_car_wash"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.NovoCarroActivity" />
        <activity android:name=".Activity.DetalhesActivity" />
        <activity android:name=".Activity.AlteraDados" />
    </application>

</manifest>